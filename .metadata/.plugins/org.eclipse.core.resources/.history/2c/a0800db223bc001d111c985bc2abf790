package ejercicio26;

public class Fill {
	
	private int[][] matriz;
	
	public Fill (int filas, int columnas) {
		matriz = new int[filas][columnas];
		
	}
		
		public void llenarMatriz() {
			for (int i = 0; i < matriz.length; i++) {
				for (int j = 0; j < matriz.length; j++)
					matriz[i][j] = (int) (Math.random() * 100);	
			}
		}
	public int[][] getMatriz() {
		return matriz;
	}
	
}
