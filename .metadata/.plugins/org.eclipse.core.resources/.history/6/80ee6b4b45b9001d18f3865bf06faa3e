package ejercicio18;

public class Main {

	public static void main(String[] args) {

		int unidad, decena, centena;
		String numeros;
		String letra = "E";
		unidad = 0;
		decena = 0;
		centena = 0;

		do {
			unidad++;
			if (unidad > 9) {
				unidad = 0;
				decena++;
			} else if (decena > 9) {
				decena = 0;
				centena++;
			}
			numeros = Integer.toString(centena) + "-" + Integer.toString(decena) + "-" + Integer.toString(unidad);

			for (int i = 0; i < 5; i++) {
				
				if (numeros.substring(i).equals("3")) {
					numeros +=  numeros.substring(i,i) + letra;
				}else {
					numeros += numeros.substring(i,i);
					}

			}
			System.out.println(numeros);

		} while (decena != 1);

	}

}
			
			
			
			
