			 * Integer.toString(decena) + " - " + " E "; System.out.println(letras); }else
			 * if (centena == 3) { letras = Integer.toString(centena)+ " - " + " E " + " - "
			 * + Integer.toString(unidad); }
			 * 
			 */

//System.out.println(centena + " - " + decena + " - " + letra);